{
  "name": "console-io",
  "version": "5.0.6",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Web console",
  "bin": {
    "console": "bin/console.js"
  },
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudcmd/console.git"
  },
  "scripts": {
    "start": "node bin/console",
    "start:dev": "NODE_ENV=development npm start",
    "build-progress": "webpack --progress",
    "build:client": "npm run build-progress -- --mode production",
    "build:client:dev": "NODE_ENV=development npm run build-progress -- --mode development",
    "build": "redrun build:*",
    "wisdom": "npm run build",
    "watcher": "nodemon -w client -w server --exec",
    "watch:lint": "npm run watcher -- 'npm run lint'",
    "watch:client": "redrun build:client -- --watch",
    "watch:client:dev": "redrun build:client:dev -- --watch",
    "lint:eslint-bin": "eslint --rule 'no-console:0' bin",
    "lint:eslint": "eslint --config .client.eslintrc server webpack.config.js",
    "lint": "redrun lint:*"
  },
  "keywords": [
    "console",
    "terminal",
    "express"
  ],
  "dependencies": {
    "currify": "^2.0.0",
    "debug": "^3.0.0",
    "express": "^4.14.0",
    "join-io": "^2.0.0",
    "rendy": "^1.1.0",
    "socket.io": "^2.0.1",
    "spawnify": "^4.0.0",
    "tildify": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "clean-css-loader": "^0.1.2",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "nodemon": "^1.11.0",
    "redrun": "^6.0.0",
    "style-loader": "^0.20.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT"
}
